  <div class='w3-container w3-content w3-center w3-padding-64' style='max-width:800px' id='band'>
      <h2 class='w3'>Sección Administrador</h2>
      <p class='w3-opacity'><i>(Prueba)</i></p>
  </div>
  <div id="login_form" class="col p-5">
    <h2 class="fw-bold text-center py-5">Registrar Editor</h2>
    <form method="post" action="/register/alta">
        <div class="mb-4">
            <label for="mail" class="form-label">Email</label>
            <input type="text" class="form-control" name="mail">
        </div>
        <div class="mb-4">
            <label for="password" class="form-label">Contraseña</label>
            <input type="password" class="form-control" name="password">
        </div>
        <input id="tipo" name="tipo" type="hidden" value="4">
        <div class="d-grid">
            <button type="submit" class="btn-secondary" style="margin: 0 40rem 0 40rem !important;">Registrar Editor</button>
        </div>
    </form>
  </div>
  <div id="login_form" class="col p-5">
    <h2 class="fw-bold text-center py-5">Registrar Escritor</h2>
    <form method="post" action="/register/alta">
        <div class="mb-4">
            <label for="mail" class="form-label">Email</label>
            <input type="text" class="form-control" name="mail">
        </div>
        <div class="mb-4">
            <label for="password" class="form-label">Contraseña</label>
            <input type="password" class="form-control" name="password">
        </div>
        <input id="tipo" name="tipo" type="hidden" value="3">
        <div class="d-grid">
            <button type="submit" class="btn-secondary" style="margin: 0 40rem 0 40rem !important;">Registrar Escritor</button>
        </div>
    </form>
  </div>
  <div id="login_form" class="col p-5">
      <h2 class="fw-bold text-center py-5">Agregar Producto</h2>
      <form method="post" action="/producto/agregar">
          <div class="mb-4">
              <label for="nombre" class="form-label">Nombre</label>
              <input type="text" class="form-control" name="nombre">
          </div>
          <div class="mb-4">
              <label for="descripcion" class="form-label">Descripcion</label>
              <input type="text" class="form-control" name="descripcion">
          </div>
          <div class="mb-4">
              <label for="logo" class="form-label">URL Logo</label>
              <input type="text" class="form-control" name="logo">
          </div>
          <div class="mb-4">
              <label for="fecha" class="form-label">Tipo</label>
              <select class="form-control" name="tipo" id="tipo">
                  {{#tiposDeProductos}}
                      <option value={{id}}>{{nombre}}</option>
                  {{/tiposDeProductos}}
              </select>
          </div>
          <div class="d-grid">
              <button type="submit" class="btn-secondary" style="margin: 0 40rem 0 40rem !important;">Agregar producto</button>
          </div>
      </form>
  </div>
  <div id="login_form" class="col p-5">
      <h2 class="fw-bold text-center py-5">Eliminar Producto</h2>
      <form method="post" action="/producto/eliminar">
          <div class="mb-4">
              <label for="fecha" class="form-label">Producto</label>
              <select class="form-control" name="id" id="id">
                  {{#productos}}
                      <option value={{id}}>{{nombre}}</option>
                  {{/productos}}
              </select>
          </div>
          <div class="d-grid">
              <button type="submit" class="btn-secondary" style="margin: 0 40rem 0 40rem !important;">Eliminar Producto</button>
          </div>
      </form>
  </div>
